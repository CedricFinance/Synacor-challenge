{
  "name": "data",
  "version": "1.0.0",
  "main": "src/main.js",
  "bin": {
    "synacor": "dist/bin/synacor.js"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "tsc",
    "start": "ts-node bin/synacor.ts run challenge.bin",
    "disass": "ts-node bin/synacor.ts disassemble challenge.bin",
    "startDisass": "ts-node bin/synacor.ts run challenge.bin --eval 'if (pc === 0x3d2) { context.disassemble(memory); exit() }'",
    "test": "mocha -r ts-node/register test/**/*.ts",
    "watch": "npm-watch"
  },
  "watch": {
    "test": {
      "patterns": ["src", "test"],
      "extensions": "ts"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bluebird": "^3.4.7",
    "caporal": "^0.3.0",
    "chalk": "^1.1.3",
    "debug": "^2.2.0",
    "inquirer": "^1.0.2",
    "sprintf": "^0.1.5"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.2",
    "@types/chai": "^3.4.35",
    "@types/chalk": "^0.4.31",
    "@types/mocha": "^2.2.40",
    "@types/node": "^7.0.10",
    "@types/sprintf": "0.0.28",
    "chai": "^3.5.0",
    "mocha": "^3.2.0",
    "npm-watch": "^0.1.8",
    "ts-node": "^3.0.2",
    "typescript": "^2.2.1"
  },
  "description": ""
}
